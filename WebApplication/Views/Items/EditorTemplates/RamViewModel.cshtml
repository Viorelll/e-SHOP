@using WebApplication.Models
@model WebApplication.Models.RamViewModel

@{
    ViewBag.Title = "Ram";
}


@Html.Hidden("ModelType", typeof(RamViewModel))

<div class="form-group col-md-12">
    @Html.EditorFor(model => model.Type, new { htmlAttributes = new { @class = "form-control", placeholder = "Type"} })
    @Html.ValidationMessageFor(model => model.Type)
</div>

<div class="form-group col-md-12">
    @Html.EditorFor(model => model.Capacity, new { htmlAttributes = new { @class = "form-control", placeholder = "Capacity"} })
    @Html.ValidationMessageFor(model => model.Capacity)
</div>

<div class="form-group col-md-12">
    @Html.EditorFor(model => model.Frequency, new { htmlAttributes = new { @class = "form-control", placeholder = "Frequency"} })
    @Html.ValidationMessageFor(model => model.Frequency)
</div>
