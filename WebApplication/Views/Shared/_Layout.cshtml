@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Home | E-Shopper</title>


    <link rel="stylesheet" href="@BundleTable.Bundles.ResolveBundleUrl("~/Content/CommonStyles")" type="text/css"/>
    <link rel="stylesheet" href="@BundleTable.Bundles.ResolveBundleUrl("~/template/css/TemplateStyles")" type="text/css" />
 

    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/template/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/template/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/template/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/template/images/ico/apple-touch-icon-57-precomposed.png">
</head><!--/head-->
<body>
    <header id="header">
        <!--header-->
        <div class="header_top">
            <!--header_top-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="contactinfo">
                            <ul class="nav nav-pills">
                                <li><a href="#"><i class="fa fa-phone"></i> +373 69 122 445</a></li>
                                <li><a href="#"><i class="fa fa-envelope"></i> info@amdaris-practice.com</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <!--/header_top-->



        <div class="header-middle">
            <!--header-middle-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-4">
                        <div class="logo pull-left">
                            <a href="~/Home/Index"><img src="~/template/images/home/logo.png" alt="" /></a>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <div class="shop-menu pull-right">
                            <ul class="nav navbar-nav">
                                <li><a href="~/Home/Login"><i class="fa fa-user"></i> Account</a></li>
                                <li><a href="~/Cart/Index"><i class="fa fa-shopping-cart"></i> Cart</a></li>
                                <li><a href="~/Home/Login"><i class="fa fa-lock"></i> Login</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div><!--/header-middle-->

        <div class="header-bottom">
            <!--header-bottom-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-9">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <div class="mainmenu pull-left">
                            <ul class="nav navbar-nav collapse navbar-collapse">
                                <li><a href="~/Home/Index" class="active">Home</a></li>
                                <li class="dropdown">
                                    <a href="#">Shop<i class="fa fa-angle-down"></i></a>
                                    <ul role="menu" class="sub-menu">
                                        <li><a href="~/Items/Shop">Products</a></li>
                                        <li><a href="~/Cart/Index">Cart</a></li>
                                        <li><a href="~/Home/Login">Login</a></li>
                                    </ul>
                                </li>
                                <li><a href="~/Home/Contact">Contact</a></li>
                                <li><a href="~/swagger/ui/index#/Items">Web API</a></li>
                                <li><a href="~/elmah">Logger</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="search_box pull-right">
                            <input id="search" type="text" placeholder="Search"/>
                        </div>
                    </div>
                </div>
            </div>
        </div><!--/header-bottom-->
</header><!--/header-->




<!--content-->
    @RenderBody()





<footer id="footer">
        <!--Footer-->

        <div class="footer-widget">
            <div class="container">
                <div class="row">
                    <div class="col-sm-2">
                        <div class="single-widget">
                            <h2>Service</h2>
                            <ul class="nav nav-pills nav-stacked">
                                <li><a href="#">Online Help</a></li>
                                <li><a href="#">Contact Us</a></li>
                                <li><a href="#">Order Status</a></li>
                                <li><a href="#">Change Location</a></li>
                                <li><a href="#">FAQ’s</a></li>
                            </ul>
                        </div>
                    </div>


                </div>
            </div>
        </div>

        <div class="footer-bottom">
            <div class="container">
                <div class="row">
                    <p class="pull-left">Copyright © 2016 E-SHOPPER | Amdaris Practice Inc. All rights reserved.</p>
                </div>
            </div>
        </div>

</footer><!--/Footer-->


<script type="text/javascript" src="@BundleTable.Bundles.ResolveBundleUrl("~/templateScripts/TempScripts")"></script>
<script src="~/scripts/jquery-1.10.2.js"></script>
<script type="text/javascript" src="@BundleTable.Bundles.ResolveBundleUrl("~/scripts/CommonScripts")"></script>


</body>
</html>


